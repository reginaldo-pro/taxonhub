{
  "name": "taxonhub",
  "version": "0.1.0",
  "description": "",
  "author": "Reginaldo RÃ©",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll",
    "test-coverage": "jest --coverage --colors",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "dev": "webpack-dev-server --env.mode development",
    "prod": "webpack --env.mode production",
    "electron-dev": "concurrently \" cross-env BROWSER=none ELECTRON_IS_DEV=1 npm run dev\" \"wait-on http://localhost:3000 && nodemon --exec \"\"electron .\"\"\"",
    "electron-prod": "npm run prod && cross-env BROWSER=none ELECTRON_IS_DEV=0 nodemon --exec \"\"electron .\"\"",
    "electron-build": "npm run prod && cross-env BROWSER=none ELECTRON_IS_DEV=0 && electron-builder"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "br.edu.utfpr.dacom-cm.taxonhub",
    "productName": "TaxonHub",
    "files": [
      "public/**/*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "linux": {
      "target": "deb",
      "category": "Science",
      "icon": "./build/favicon.ico"
    },
    "windows": {
      "target": "nsis",
      "category": "Science",
      "icon": "./build/favicon.ico"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/__config__/enzyme.config.js.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/src/tests/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/webpack-env": "^1.15.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "electron-reload": "^1.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.7",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-hot-loader": "^4.12.20",
    "react-loadable": "^5.5.0",
    "regenerator-runtime": "^0.13.9",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^4.2.3",
    "tslint": "^6.1.1",
    "tslint-immutable": "^6.0.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "wait-on": "^4.0.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.10",
    "electron-is-dev": "^1.1.0",
    "papaparse": "^5.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
